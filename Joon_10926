public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int A,B,C,D,E,F;
		int a,b,c,d,e,f;
		
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		D = sc.nextInt();
		E = sc.nextInt();
		F = sc.nextInt();
		
		a = 1 - A;
		b = 1 - B;
		c = 2 - C;
		d = 2 - D;
		e = 2 - E;
		f = 8 - F;
		
		System.out.printf("%d %d %d %d %d %d" ,a,b,c,d,e,f);
	}
}
